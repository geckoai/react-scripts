  "description": "react-scripts",
  "main": "dist",
  "types": "dist/types",
  "bin": {
    "react-scripts": "./dist/bin/index.js"
  },
  "scripts": {
    "build:clean": "rimraf dist && rimraf docs",
    "build:cjs": "tsc --project tsconfig.prod.cjs.json",
    "build:types": "tsc --project tsconfig.prod.types.json",
    "build:docs": "typedoc --options ./typedoc.json",
    "release": "yarn build:clean && yarn build:cjs",
    "bin:start": "ts-node ./src/bin/index.ts start",
    "bin:start2": "node --trace-warnings ./dist/bin/index.js start",
    "bin:build": "ts-node ./src/bin/index.ts build"
  },
